{
  "name": "kamrad",
  "version": "1.0.0",
  "description": "Kuadrant developer portal",
  "main": "src/js/index.ts",
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:hugo": "hugo server -v -D --disableFastRender",
    "start:webpack": "webpack serve --config webpack.dev.js --hot",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:hugo": "hugo -v",
    "build:hugo:preview": "npm run build:hugo -- -D -F",
    "build:webpack": "NODE_ENV=production webpack --config webpack.prod.js",
    "lint": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3scale-labs/kamrad.git"
  },
  "keywords": [
    "api",
    "api",
    "management",
    "dev",
    "portal",
    "documentation",
    "kubernetes"
  ],
  "author": "didierofrivia",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/3scale-labs/kamrad/issues"
  },
  "homepage": "https://github.com/3scale-labs/kamrad#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "@types/swagger-ui": "^3.47.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.2.0",
    "css-loader": "^6.0.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^2.1.0",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
